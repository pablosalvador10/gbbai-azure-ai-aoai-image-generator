{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "resourceGroupName": {
        "value": "${AZURE_APP_RG}"
      },
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "gptModel": {
        "value": "${AZURE_GPT_MODEL}"
      },
      "gptVersion": {
        "value": "${AZURE_GPT_VERSION}"
      },
      "openaiName": {
        "value": "${AZURE_OPENAI_NAME}"
      },
      "tags": {
        "value": {
          "Owner": "GBB Team",
          "Project": "GPTContentGeneration",
          "Environment": "Dev",
          "Toolkit": "Bicep"
        }
      }
    }
  }